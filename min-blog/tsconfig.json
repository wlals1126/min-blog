{
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "lib": [
      "ES2020",
      "DOM"
    ],
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "strict": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "downlevelIteration": true,
    "paths": {
      "@your_directory/*": [
        "your_directory/*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "isolatedModules": true,
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "styles/default.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
